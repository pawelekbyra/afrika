{
  "version": 2,
  "builds": [
    {
      "src": "pro2-feat-ting-tong-migration/server/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "pro2-feat-ting-tong-migration/client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "outputDirectory": "client/dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/pro2-feat-ting-tong-migration/server/index"
    },
    {
      "source": "/assets/(.*)",
      "destination": "/pro2-feat-ting-tong-migration/client/dist/assets/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/pro2-feat-ting-tong-migration/client/dist/index.html"
    }
  ]
}